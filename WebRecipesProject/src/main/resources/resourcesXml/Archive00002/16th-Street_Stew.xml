<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>16th-Street Stew</title>
      <categories>
        <cat>Crs</cat>
        <cat>Totest</cat>
        <cat>Post</cat>
        <cat>Chopmeat</cat></categories>
      <yield>4</yield></head>
    <ingredients>
      <ing>
        <amt>
          <qty>1 1/2</qty>
          <unit>pounds</unit></amt>
        <item>Ground beef</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit>medium</unit></amt>
        <item>Chopped onion</item></ing>
      <ing>
        <amt>
          <qty>4</qty>
          <unit>slices</unit></amt>
        <item>Bacon diced</item></ing>
      <ing>
        <amt>
          <qty>1</qty>
          <unit/></amt>
        <item>Chopped green pepper</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit/></amt>
        <item>Potatoes peeled and diced</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>teaspoons</unit></amt>
        <item>Mustard</item></ing>
      <ing>
        <amt>
          <qty>3</qty>
          <unit/></amt>
        <item>Beef bouillon cubes</item></ing>
      <ing>
        <amt>
          <qty>1/4</qty>
          <unit>teaspoons</unit></amt>
        <item>Pepper</item></ing>
      <ing>
        <amt>
          <qty>2</qty>
          <unit>cups</unit></amt>
        <item>Boiling water</item></ing>
      <ing>
        <amt>
          <qty>6</qty>
          <unit>tablespoons</unit></amt>
        <item>Vineger</item></ing></ingredients>
    <directions>
      <step>  Brown meat, onion, bacon and green pepper; drain. Stir in remaining
  ingredients. Simmer unitl potatoes are tender over low heat, about 45 mins.
  
