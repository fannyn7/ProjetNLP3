<?xml version="1.0" encoding="UTF-8"?>

<recipeml version="0.5">
  <recipe>
    <head>
      <title>( From Bread Mix ) Homemade White Bread</title>
      <categories>
        <cat>Breadmaker</cat>
        <cat>Mixes</cat>
        <cat>Breads</cat></categories>
      <yield>1</yield></head>
    <ingredients>
      <ing-div>
        <title/>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>tablespoon</unit></amt>
          <item>Yeast</item></ing>
        <ing>
          <amt>
            <qty>2</qty>
            <unit>tablespoons</unit></amt>
          <item>Vegetable oil lukewarm water</item></ing>
        <ing>
          <amt>
            <qty>3 1/2</qty>
            <unit>cups</unit></amt>
          <item>Bread mix</item></ing>
        <ing>
          <amt>
            <qty>1</qty>
            <unit/></amt>
          <item>Egg; beaten</item></ing></ing-div>
      <ing-div>
        <title>VARIATIONS</title>
        <ing>
          <amt>
            <qty>1</qty>
            <unit>package</unit></amt>
          <item>Dry onion soup mix makes great hamburger buns OR</item></ing>
        <ing>
          <amt>
            <qty>4</qty>
            <unit>tablespoons</unit></amt>
          <item>Brown sugar; and</item></ing>
        <ing>
          <amt>
            <qty>2/3</qty>
            <unit>cups</unit></amt>
          <item>Wheat germ</item></ing></ing-div></ingredients>
    <directions>
      <step>  I have a lot more of the homemade mix recipes if anyone is interested.
  
  From Gemini's MASSIVE MealMaster collection at www.synapse.com/~gemini
 
</step></directions></recipe></recipeml>
